{
  "name": "create-nitro-app",
  "version": "0.8.4",
  "description": "Create a Nitro app with a single command",
  "repository": "nitrojs/create-nitro-app",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "bin": "./dist/cli.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "obuild",
    "create-nitro-app": "node ./src/cli.ts",
    "lint": "eslint . && prettier -c .",
    "lint:fix": "automd && eslint . --fix && prettier -w .",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types",
    "test:types": "tsgo --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@typescript/native-preview": "7.0.0-dev.20260120.1",
    "automd": "^0.4.2",
    "changelogen": "^0.6.2",
    "citty": "^0.1.6",
    "consola": "^3.4.2",
    "eslint": "^9.39.2",
    "eslint-config-unjs": "^0.6.2",
    "giget": "^3.0.0",
    "nypm": "^0.6.3",
    "obuild": "^0.4.18",
    "prettier": "^3.8.0",
    "tinyexec": "^1.0.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "packageManager": "pnpm@10.28.1"
}
